{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/obj": true,
        "**/bin": true
    },
    "sqlfluff.dialect": "sqlite",
    "sqlfluff.suppressNotifications": true,
    "sqlfluff.config": ".sqlfluff",
    "sqlfluff.linter.run": "off",
    "[csharp]": {
        "editor.maxTokenizationLineLength": 2500
    }
}
